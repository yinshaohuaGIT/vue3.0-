    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "jest": "^26.6.3",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "ts-jest": "^26.5.6",
    "typescript": "^4.8.2",
    "vite": "^3.0.4",
