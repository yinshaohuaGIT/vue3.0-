    "@types/jest": "^29.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
